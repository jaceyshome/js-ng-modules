div.geophysics-container.scenario-decision-interactive-container
  .top-interactive-container(data-ng-style="getTopContainerMinHeight()")

    div.text-panel.topContainerFirstElement.animate-left(data-ng-if="screen.id == 0", data-anim-easing="easeOutCubic", data-anim-duration="600", data-key="{{settings.dataKey}}")
      .row
        .col-md-12
          .introduction-text-body
            p {{settings.screens[0].text}}
          .button-container.open
            a.show-next-button(href="javascript:void(0);", onclick="return false;", data-ng-click="goToMageneticView()", class="{{Structure.appendStyles.interactiveHoverButtonStyles[Structure.data.currentModuleKey]}}", data-ng-style="{'border-color':Module.currentModule.color}") view magnetic results
    div.text-panel.animate-left(data-ng-if="screen.id == 1 && screen.feedback != true", data-anim-easing="easeOutCubic", data-anim-duration="600", data-key="{{settings.dataKey}}")
      .row
        .col-md-12
          .introduction-text-body
            p {{settings.screens[1].text}}
    div.text-panel.animate-left(data-ng-if="screen.id == 1 && zone.id == 0", data-anim-easing="easeOutCubic", data-anim-duration="600", data-key="{{settings.dataKey}}")
      p.feedback(data-ng-style="{'background-color':'#777674'}")
        span.sr-only feedback:
        span {{settings.zones[0].magnetic.correct && "correct" || "incorrect"}}
      .row
        .col-md-12
          .introduction-text-body
            p {{settings.zones[0].magnetic.feedback}}
    div.text-panel.animate-left(data-ng-if="screen.id == 1 && zone.id == 1", data-anim-easing="easeOutCubic", data-anim-duration="600", data-key="{{settings.dataKey}}")
      p.feedback(data-ng-style="{'background-color':Module.currentModule.color}")
